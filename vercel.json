{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "app.js"
      },
      {
        "src": "/static/(.*)",
        "dest": "/frontend/dist/$1"
      },
      {
        "src":"/(.*)",
        "dest":"/frontend/dist/index.html"
      }
    ]
  }
  